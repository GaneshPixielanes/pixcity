        var coordinates = {};
				$(document).ready(function()
				{
					var url = window.location.href;
					var agendas = false;
					var city_maker = false;
					var parameter = false;

					url = url.split('#');
					if("undefined" != typeof url[1])
					{
						if(url[1] == "favoris")
						{
							console.log('wroe');
							$('a[href="#mes-city-makers"]').trigger('click');
						}
					}

					// Load other pages via ajax

					$('.load-page-content').click(function()
					{
						console.log('clicked');
						var link = $(this).attr('data-href');
						var id = $(this).attr('id');
						console.log(id);
						$.ajax({
							url:link,
							method: 'POST',
							success: function(e)
							{
								if( id == "mes-cards-attente-page"  && agendas == false)
								{
									$("#mes-agendas-container").html(e);
										agendas = true;
								}
								else if( id == "mes-agendas-page" && agendas == false)
								{
									$("#mes-agendas-container").html(e);
									agendas = true;
								}
								else if( id == "mes-city-maker" && city_maker == false)
								{

									$("#mes-city-makers-container").html(e);
									city_maker = true;
								}
								else if( id == "settings-page" && parameter == false)
								{

									$("#settings-container").html(e);
									$('.pc-table-payment').DataTable();
									$('.dataTables_filter > label, .dataTables_paginate').addClass('float-right');
									parameter = true;
								}
							}
						});
