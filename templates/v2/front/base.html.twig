<body class="{% block body_class %}{% if not is_granted('IS_AUTHENTICATED_FULLY') %}logged-out{% else %}logged-in{% endif %} {% endblock %}">
<nav class="navbar navbar-expand-lg fixed-top nav-bar-main">
    <div class="container">
        <a class="navbar-brand" href="{{ path('front_homepage_index') }}"><img src="{{ absolute_url(asset('build/images/header-logo-main.svg')) }}" alt="Pix.City"></a>
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarTogglerDemo03" aria-controls="navbarTogglerDemo03" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
            <img src="{{ absolute_url(asset('build/images/menu.svg')) }}" />
        </button>
        <div class="collapse navbar-collapse" id="navbarTogglerDemo03">
            <div class="col-md-5 ml-auto mt-2 mt-lg-0 pr-0">
                <div class="row">
                  {% if not is_granted('IS_AUTHENTICATED_FULLY') %}
                    <div class="col-md-7">
                        <button class="btn btn-rollover btn-outline-gray btn-pill btn-block btn-icon-right my-2 my-sm-0 city-maker" type="submit" id="cityMakerRegisterButton"><span class="text">Devenir City-Maker</span><i class="fas fa-user-plus"></i></button>
                    </div>
                    <div class="col-md-5">
                        <button class="btn btn-rollover btn-green btn-pill btn-block my-2 my-sm-0" type="submit"><span class="text">Connexion</span><i class="fas fa-sign-in-alt"></i></button>
                    </div>
                {% else %}
                <div class="col-md-7">
                </div>
                <div class="col-md-5">
                    <button id="logout" class="btn btn-rollover btn-green btn-pill btn-block my-2 my-sm-0"><span class="text">Logout</span><i class="fas fa-sign-in-alt"></i></button>
                </div>
                {% endif %}
            </div>
            </div>
        </div>
    </div>
</nav>
