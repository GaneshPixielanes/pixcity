<form class=" pull-right" method="post" action="{{ path('admin_user_packs_delete', {'id': user_pack.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ user_pack.id) }}">
    <button class="btn"><i class="material-icons">delete</i>Delete</button>
</form>
