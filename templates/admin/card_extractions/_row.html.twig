<tr data-id="{{ item.id }}">
    <td>{{ item.name ? item.name : ''}}</td>
    <td>{{ "https://www.pix.city/"~item.slug~".html" }}</td>
    <td>{{ item.region.name }}</td>
    <td>{{ item.department.name }}</td>
    <td>{{ item.address.city ? item.address.city : ''}}</td>
    <td></td>
    <td>{{ item.createdAt|date('Y-m-d') }}</td> 
	<td><a title="{{ 'action.edit'|trans }}" href="{{ path('admin_cards_edit', {id: item.id}) }}" class="btn btn-circle bg-pink waves-effect waves-circle waves-float">
                <i class="material-icons">edit</i>
            </a></td> 
</tr>