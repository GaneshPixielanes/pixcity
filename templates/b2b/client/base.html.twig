        $(document).on('click','#terminateMission',function () {

            var amount = $(this).attr('data-adjust-amount');
            var type = $(this).attr('data-type');
            var id = $(this).attr('data-id');

            $.ajax({
                url:"{{ path('b2b_client_main_mission_details') }}",
                data:{id:id},
                method:'GET',
                success:function (data) {
                    console.log(amount);
                    if(amount > 0){
                        if(type == 'terminate'){
                            $("#break-up-text").text('The Breakup amount need to pay to terminate the mission');
                            $("#price").text('Price :'+parseFloat(data.price).toFixed(2));
                            $("#tax").text("Vat :"+parseFloat(data.tax).toFixed(2));
                            $('#total').text("Total :"+parseFloat(data.total).toFixed(2));
                            $("#advance-payment").text('Advance Payment:'+parseFloat(data.advance_payment).toFixed(2));
                            $("#need-to-pay").text('Advance Payment:'+parseFloat(data.need_to_pay).toFixed(2));
                            $("#mission-terminate").attr("href","/client/mission/process-mission-request/"+id);
                            $('#mission-terminate').attr('data-type','terminate');
                            $(".teminate-class").removeAttr('id','none');

                        }else if(type == 'cancel'){
                            var cancel_amount = parseFloat(data.advance_payment).toFixed(2);
                            var calcPrice = cancel_amount - ( (cancel_amount/100) * 2 );

                            $("#break-up-text").text('The Breakup amount need to pay to terminate the mission');
                            $("#price-cancel").text('Price :'+parseFloat(data.price).toFixed(2));
                            $("#tax-cancel").text("Vat :"+parseFloat(data.tax).toFixed(2));
                            $('#total-cancel').text("Total :"+parseFloat(data.total).toFixed(2));
                            $("#advance-payment-cancel").text('Advance Payment:'+parseFloat(data.advance_payment).toFixed(2));
                            $("#need-to-pay-cancel").text('if you cancelled the Mission we will refund you:'+calcPrice.toFixed(2)+' amount to your account,');

                            $("#mission-terminate-cancel").attr("href","/client/api/mission/status/"+id+'/'+type);
                            $('#mission-terminate-cancel').attr('data-type','terminate');
                            $(".teminate-class-cancel").removeAttr('id','none');
                        }

                    }else if(amount < 0){
                        console.log(type);

                        if(type == 'terminate'){
                            $("#break-up-text").text('The Refundable amount will be done once you terminate the mission');
                            $("#price").text('Price :'+parseFloat(data.price).toFixed(2));
                            $("#tax").text("Vat :"+parseFloat(data.tax).toFixed(2));
                            $('#total').text("Total :"+parseFloat(data.total).toFixed(2));
